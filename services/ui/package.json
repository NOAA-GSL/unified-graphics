{
  "name": "ui",
  "version": "0.0.1",
  "scripts": {
    "clean": "del build",
    "dev": "run-s clean dev:*",
    "dev:vite": "vite dev",
    "build": "run-s clean build:*",
    "build:vite": "vite build",
    "preview": "vite preview",
    "start": "npm run dev",
    "test": "vitest --run --coverage",
    "lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. . && eslint --ignore-path .gitignore . && stylelint --ignore-path .gitignore src/**/*.{css,scss}",
    "format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. . && stylelint --ignore-path .gitignore --fix src/**/*.{css,scss}"
  },
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "^1.4.0",
    "@uswds/uswds": "^3.0.0",
    "@vitest/coverage-c8": "^0.23.4",
    "c8": "^7.12.0",
    "d3": "^7.4.4",
    "del-cli": "^5.0.0",
    "eslint": "^8.35.0",
    "eslint-config-prettier": "^8.7.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "jsdom": "^21.1.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.4",
    "prettier-plugin-svelte": "^2.9.0",
    "sass": "^1.51.0",
    "stylelint": "^14.8.2",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-recommended-scss": "^7.0.0",
    "stylelint-order": "^6.0.2",
    "stylelint-prettier": "^3.0.0",
    "stylelint-scss": "^4.2.0",
    "svelte": "^3.50.1",
    "vite": "^3.0.0",
    "vite-plugin-static-copy": "^0.13.1",
    "vitest": "^0.29.1"
  },
  "type": "module"
}
